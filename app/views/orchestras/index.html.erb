<h1>All orchestras</h1>
<% @orchestras.each do |orchestra| %>
  <h2><%= orchestra.name %> </h2>
  <h3><a href="/orchestras/<%= orchestra.id %>"><%= orchestra.name %></a> created at:<%=orchestra.created_at%></h3>
  <p><%= link_to "Orchestra Information", "orchestra/#{orchestra.id}"%></p>
<% end %>
