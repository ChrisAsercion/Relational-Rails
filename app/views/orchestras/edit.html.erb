<form action="/orchestras" method="patch">
  <input type="hidden" name="authenticity_token" value="<%= form_authenticity_token %>">
  <p>Enter information for <%= @current_group.name%></p>
  <p>name:</p><input type='text' name='orchestra[name]'/><br/>
  <p>chairs:</p><input type='number' name='orchestra[chairs]'/><br/>
  <p>conductor:</p><input type='text' name='orchestra[conductor]'/><br/>
  <p>active orchestra:</p><input type='checkbox' name='orchestra[active]'/><br/>
  <p>orchestra has a venue:</p><input type='checkbox' name='orchestra[venue]'/><br/>
  <input type='submit'/>
</form>